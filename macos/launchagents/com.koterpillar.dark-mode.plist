<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.koterpillar.dark-mode</string>
    <key>KeepAlive</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
       <string>/bin/sh</string>
       <string>-c</string>
       <string>$HOME/.local/bin/dark-notify -c $HOME/.local/bin/dark-mode</string>
    </array>
</dict>
</plist>
